{
  "name": "web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:check": "astro check && astro build",
    "check": "astro check --root ./src && echo \"astro check passed\"",
    "serve": "npx serve ./dist --listen 3210",
    "preview": "astro preview",
    "astro": "astro",
    "reinstall": "rm -rf node_modules && npm install",
    "npm:install": "rm -rf node_modules && rm -rf dist && npm cache clean --force && npm install",
    "npm:reinstall": "rm -rf node_modules && rm -rf dist && rm -f package-lock.json && npm cache clean --force && npm install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/svelte": "^5.0.3",
